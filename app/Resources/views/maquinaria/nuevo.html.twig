{% block cuerpo %}
    
    <div class="col-md-10">

        
        <div class="row" style=" margin-left:0px; margin-right:0px;">
            <section class="col-lg-12 connectedSortable ui-sortable">
                <div class="nav-tabs-custom" style="cursor: move;">
                    <ul class="nav nav-tabs  ui-sortable-handle">
                        <li class="active"><a href="#divDatosGenerales" data-toggle="tab" class="colorAnclas colorPestanhas">Datos Generales</a></li>
                        <li><a href="#divdatosMantenimiento" data-toggle="tab" class="colorAnclas colorPestanhas">Datos del mantenimiento</a></li>
                        <li><a href="#divExpedienteMantenimiento" data-toggle="tab" class="colorAnclas colorPestanhas">Expediente Mantenimiento</a></li>
                        <li><a href="#divImagenesMaquina" data-toggle="tab" class="colorAnclas colorPestanhas">Imagenes</a></li>
                        <li class="pull-left header">
                    </ul>
                    <div class="tab-content no-padding">

                        <div class="chart tab-pane active colorDiv" id="divDatosGenerales" >
                            <div class="row" style="margin-left: 2%; ">
                                <div class="form-column col-md-4">
                                    <div class="form-group">
                                        <label for="numeroSerie" class="control-label" style="padding-top: 1.5%;">Numero Serie</label>
                                        <input type="text" class="form-control" id="numeroSerie" 
                                               placeholder="N°  de serie" name="numeroSerie" >
                                    </div>
                                </div>
                    
                                 <div class="form-column col-md-4">
                                    <div class="form-group" >
                                        <label for="numeroEquipo" class="control-label" style="padding-top: 1.5%;">N° de equipo</label>
                                        <input type="text" class="form-control" id="numeroEquipo" 
                                               placeholder="Numero de Equipo" name="numeroEquipo" >
                                    </div>
                                </div>
                                  <div class="form-column col-md-4" style="padding-top: 3.2%;">
                                    <div class="form-group">
                                        <label for="anhoMaquina" class="control-label">Año</label>
                                        <input type="text" name="anhoMaquina" id="anhoMaquina" >
                                        
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                                 <div class="form-column col-md-4">
                                    <div class="form-group" >
                                        <label for="alias   " class="control-label">Alias</label>
                                        <input type="text" class="form-control" id="alias" 
                                               placeholder="Alias de la maquina" name="alias" >
                                    </div>
                                </div>
                                <div class="form-column col-md-4">
                                    <div class="form-group" >
                                        <label for="modelo   " class="control-label">Modelo</label>
                                        <input type="text" class="form-control" id="modelo" 
                                               placeholder="Modelo de la maquina" name="modelo" >
                                    </div>
                                </div>
                                 <div class="form-column col-md-4">
                                     <div class="form-group" style="margin-right: 2%;">
                                        <label for="tipoEquipo" class="control-label">Tipo equipo</label>
                                        <select id="tipoEquipo" name="tipoEquipo" class="form-control" style="width: 100%">
                                            <option value="" disabled selected>Seleccione un tipo equipo ...</option>
                                        </select>  
                                            
                                    </div>
                                </div>
                                 <div class="clearfix"></div>
                                 <div class="form-column col-md-4">
                                    <div class="form-group" >
                                        <label for="vin   " class="control-label">VIN</label>
                                        <input type="text" class="form-control" id="vin" 
                                               placeholder="VIN de la maquina" name="vin" >
                                    </div>
                                </div>
                                <div class="form-column col-md-4">
                                    <div class="form-group" >
                                        <label for="placa   " class="control-label">Placa</label>
                                        <input type="text" class="form-control" id="placa" 
                                               placeholder="N° de placa" name="placa" >
                                    </div>
                                </div>
                                 <div class="form-column col-md-4">
                                    <div class="form-group" style="margin-right: 2%;">
                                        <label for="color   " class="control-label" >Color</label>
                                        <select id="colorMaquina" name="colorMaquina" style="width: 100%">
                                            <option value="seleccione" >..::Seleccione un valor::..</option>
                                            <option value="Negro" >Negro</option>
                                            <option value="Naranja" >Naranja</option>
                                            <option value="Celeste" >Celeste</option>
                                            <option value="Azul" >Azul</option>
                                            <option value="Blanco" >Blanco</option>
                                             <option value="Cafe" >Cafe</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                                 <div class="form-column col-md-4">
                                    <div class="form-group" >
                                        <label for="tamanho" class="control-label">Tamaño</label>
                                        <input type="text" class="form-control" id="tamanho" 
                                               placeholder="Tamaño de la maquina" name="tamanho" >
                                    </div>
                                </div>
                                 <div class="form-column col-md-4">
                                    <div class="form-group" >
                                        <label for="capacidad" class="control-label">Capacidad</label>
                                        <input type="text" class="form-control" id="capacidad" 
                                               placeholder="Capacidad de la maquina" name="capacidad" >
                                    </div>
                                </div>
                                <div class="form-column col-md-4">
                                    <div class="form-group" style="margin-right: 2%;" >
                                        <label for="marca" class="control-label" >Marca</label>
                                        <input type="text" class="form-control" id="marca" 
                                               placeholder="Marca del equipo" name="marca" >
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                                <div class="form-column col-md-6">
                                    <div class="form-group" >
                                        <label for="marca" class="control-label" >Descripción</label>
                                        <textarea name="descripcionMaquina" class="form-control">
                                            
                                        </textarea>
                                    </div>
                                </div>
                                  <div class="form-column col-md-4">
                                    <div class="form-group" >
                                        <div class="btn-group pull-right">
                                            <a class="btn btn-default  btn-sm " style="margin-left: 5px;margin-top: 35px;" id="cancelarDatosGeneralesMaquina">Cancelar</a>
                                        </div>
                                        <div class="btn-group pull-right">
                                            <button class="btn btn-primary  btn-sm " style="margin-left: 5px;margin-top: 35px;" id="guargarDatosGeneralesMaquina">Guardar</button>
                                        </div>
                                    </div>
                                </div>
                                
                            </div>                          
                        </div>
                        
                        
                        <div class="chart tab-pane" id="divdatosMantenimiento" style=" padding: 15px;" >
                            
                        </div>
                         <div class="chart tab-pane" id="divExpedienteMantenimiento" style=" padding: 15px;" >
                            
                        </div>
                         <div class="chart tab-pane" id="divImagenesMaquina" style=" padding: 15px;" >
                            
                        </div>
                    </div>
                </div>
            </section>
        </div>






    </div>


    
{% endblock %}

       </div>
{% endblock %}
 
